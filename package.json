{
  "name": "something-to-remember",
  "version": "1.0.0",
  "description": "this application is a wedding planner we will help you to manage your dream  day.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">=13.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elhamposhtiban/Something-To-Remember.git"
  },
  "keywords": [
    "react",
    "express",
    "mongoose",
    "wedding",
    "Sass"
  ],
  "author": "Elham poshtiban & Makah abutahoun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elhamposhtiban/Something-To-Remember/issues"
  },
  "homepage": "https://github.com/elhamposhtiban/Something-To-Remember#readme",
  "dependencies": {
    "@appbaseio/reactivesearch": "^3.4.3",
    "@auth0/auth0-spa-js": "^1.6.4",
    "auth0-js": "^9.12.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bulma": "^0.8.0",
    "connect-mongo": "^3.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "gravatar": "^1.8.0",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "query-string": "^6.11.1",
    "validator": "^12.2.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
