{
  "name": "something-to-remember",
  "version": "1.0.0",
  "description": "this application is a wedding planner we will help you to manage your dream  day.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elhamposhtiban/Something-To-Remember.git"
  },
  "keywords": [
    "react",
    "express",
    "mongoose",
    "wedding",
    "Sass"
  ],
  "author": "Elham poshtiban & Makah abutahoun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elhamposhtiban/Something-To-Remember/issues"
  },
  "homepage": "https://github.com/elhamposhtiban/Something-To-Remember#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.9.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
